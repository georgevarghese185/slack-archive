{
  "name": "slack-archive",
  "version": "0.1.0",
  "description": "A webapp to backup content in a Slack Workspace",
  "scripts": {
    "start:dev": "ENV=dev npm-run-all -l devDb:start --parallel mockSlack:start express:start",
    "test:unit": "mocha test/unit/**/*.spec.js",
    "test:integration": "mocha --config .mocharc.integ.json",
    "devDb:start": "sh ./scripts/start-db-dev.sh",
    "devDb:stop": "sh ./scripts/stop-db-dev.sh",
    "devDb:clear": "sh ./scripts/clear-db-dev.sh",
    "devDb:delete": "sh ./scripts/delete-db-dev.sh",
    "mockSlack:start": "nodemon -w test/mockSlack --require dotenv/config test/mockSlack",
    "mockSlack:generate": "node test/mockSlack/data/generator/generate.js",
    "express:start": "nodemon -w src -w .env --require dotenv/config src/server/express"
  },
  "author": "George Varghese M",
  "license": "UNLICENSE",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^7.2.0",
    "moxios": "^0.4.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "chai-sorted": "^0.2.0",
    "cookie": "^0.4.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "deep-equal-in-any-order": "^1.0.27",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "query-string": "^6.10.1",
    "sequelize": "^5.21.10",
    "uuid": "^7.0.1"
  }
}
